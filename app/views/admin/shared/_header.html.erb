<header>
  <span class="logo-mark">BAUKIS</span>
  <%= link_to 'BAUKIS', :admin_root, class: 'logo-mark' %>
  <%= content_tag(:span, flash.notice, class: 'notice') if flash.notice %>
  <%= content_tag(:span, flash.alert, class: 'alert') if flash.alert %>
  <%=
    if current_administrator
      #binding.pry
      link_to 'ログアウト', admin_session_path(current_administrator), method: :delete
      #:admin_session,method: :delete
    else
      #binding.pry
      link_to 'ログイン', :admin_login
    end
  %>
</header>
